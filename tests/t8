;----- Testcase 4. By Fomenko: L1->L2, L2->L3, L1->L4. -----
N40 (DIS, "RAZVERTKA 4 OTV")
G0 G29 G40 G90 G79 Z0
G95
(UAO,2)
(URT,0)
;RAZVERTKA D12
T4.4 M16
S3180 M3 M8


"L1"
  ;FIRST HOLE
  G0 X100 Y100
  Z3
  G1 Z-2 F0.1 
  G0 Z100
"L2"
  ;THIRD HOLE
  G0 X-100 Y-100
  Z3
  G1 Z-2 F0.1 
  G0 Z100
"L3"
  ;FIRST AND THIRD HOLES AGAIN
  ;FIRST HOLE
  G0 X100 Y100
  Z3
  G1 Z-2 F0.1 
  G0 Z100
  ;THIRD HOLE
  G0 X-100 Y-100
  Z3
  G1 Z-2 F0.1 
  G0 Z100
"L4"

;FIRST HOLE ONLY
(EPP,L1,L2)
;SECOND HOLE ONLY
(EPP,L2,L3)
;EACH HOLE BY TWO TIMES
(EPP,L1,L4)
G79 Z0 M9
G79 X0 Y0 M5
M2



;----- PROCEDURES -----



;CENTRAL HOLE PROCESSING
"CENBEG"
  G0 X0 Y0
  Z3
  G1 Z0 F0.5
  G41 X5
  G3 G40 X5 Y0 Z-5 R5 K5
  G1 X-5
  G0 Z100
"CENEND"
M2

